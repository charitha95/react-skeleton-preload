{
  "name": "@charie/react-skeleton-preload",
  "version": "0.0.0-development",
  "description": "light weight react skeleton preloder",
  "main": "./dist/bundle.cjs.js",
  "module": "./dist/bundle.esm.js",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "prebuild": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "react",
    "skeleton",
    "preloader",
    "loading",
    "loading-animation",
    "react-loading"
  ],
  "author": "charitha goonewardena",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/charitha95/react-skeleton-preload/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/charitha95/react-skeleton-preload#readme",
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-proptypes": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/charitha95/react-skeleton-preload.git"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "node-sass": "^4.14.1",
    "postcss": "^7.0.30",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "semantic-release": "^17.0.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
